<Defs>
  <PipeSystem.ProcessDef Abstract="True" Name="BOR_ProcessGenIIITemp">
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
    <temperatureRuinable>true</temperatureRuinable>
    <minSafeTemperature>-30.0</minSafeTemperature>
    <maxSafeTemperature>-15.0</maxSafeTemperature>
    <researchPrerequisites>
      <li>BOR_Research_NSynth_III</li>
    </researchPrerequisites>
  </PipeSystem.ProcessDef>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIIITemp">
    <defName>BOR_NeutroniumIntoSteelGenIIITempRuin</defName>
    <label>Fabricate Steel from Neutronium within safe temperature (-30*C - -15*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>1000</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>150000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


