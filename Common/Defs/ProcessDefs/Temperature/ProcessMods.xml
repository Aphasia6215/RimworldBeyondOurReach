<?xml version="1.0" encoding="UTF-8"?>
<Defs>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenITemp" MayRequire="mlie.infinityrimariadna">
    <defName>BOR_NeutroniumIntoTeseumGenITempRuin</defName>
    <label>Fabricate Teseum from Neutronium within safe temperature (0*C - 15*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>50000</countNeeded>
      </li>
      <li>
        <thing>Teseum</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>600000</ticks>
    <results>
      <li>
        <thing>Teseum</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>

