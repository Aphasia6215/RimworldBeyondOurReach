  <PipeSystem.ProcessDef Abstract="True" Name="BOR_ProcessGenIITemp">
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
    <temperatureRuinable>true</temperatureRuinable>
    <minSafeTemperature>-15.0</minSafeTemperature>
    <maxSafeTemperature>0.0</maxSafeTemperature>
    <researchPrerequisites>
      <li>BOR_Research_Refinery_II_Temperature_Ruinable</li>
    </researchPrerequisites>
  </PipeSystem.ProcessDef>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIITemp">
    <defName>BOR_NeutroniumIntoSteelGenIITempRuin</defName>
    <label>Fabricate Steel from Neutronium within safe temperature (-15*C - 0*C)</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>15</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>200000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


