<Defs>

  <PipeSystem.ProcessDef>
    <defName>BOR_NeutroniumIntoSteelGenI</defName>
    <label>Fabricate Steel from Neutronium</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>50</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>300000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
  </PipeSystem.ProcessDef>


