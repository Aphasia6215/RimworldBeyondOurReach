    <!-- Bionic eye -->

    <HediffDef ParentName="AddedBodyPartBase">
        <defName>BOR_SolarBionicEye</defName>
        <label>solar bionic eye</label>
        <labelNoun>a solar bionic eye</labelNoun>
        <description>An installed solar bionic eye. With its biogel nerve-link, internal signal pre-processor, and wide-spectrum sensors, it is better than a biological eye in almost every way. A lattice-dust healing system allows it to recover from damage.</description>
        <descriptionHyperlinks>
            <ThingDef>BOR_SolarBionicEye</ThingDef>
        </descriptionHyperlinks>
        <spawnThingOnRemoved>BOR_SolarBionicEye</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.875</partEfficiency>
            <betterThanNatural>true</betterThanNatural>
        </addedPartProps>
    </HediffDef>

