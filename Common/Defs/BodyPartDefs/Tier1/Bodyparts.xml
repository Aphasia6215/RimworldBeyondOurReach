    <!-- Bionic stomach -->

    <HediffDef ParentName="AddedBodyPartBase">
        <defName>BOR_SolarBionicStomach</defName>
        <label>solar bionic stomach</label>
        <labelNoun>a solar bionic stomach</labelNoun>
        <description>An installed solar bionic stomach. A person with this has a much lower chance of getting food poisoning, and is immune to some stomach diseases. An integrated lattice-dust healing system automatically repairs any damage to the artificial organ. It is better than a biological stomach in almost every way.</description>
        <descriptionHyperlinks>
            <ThingDef>BOR_SolarBionicStomach</ThingDef>
        </descriptionHyperlinks>
        <spawnThingOnRemoved>BOR_SolarBionicStomach</spawnThingOnRemoved>
        <stages>
            <li>
                <makeImmuneTo>
                    <li>GutWorms</li>
                </makeImmuneTo>
                <foodPoisoningChanceFactor>0.75</foodPoisoningChanceFactor>
            </li>
        </stages>
