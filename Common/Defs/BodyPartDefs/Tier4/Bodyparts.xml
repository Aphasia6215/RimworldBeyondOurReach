		<skillRequirements>
			<Medicine>20</Medicine>
			<Intellectual>20</Intellectual>
		</skillRequirements>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>BOR_MedicineIII</li>
					</thingDefs>
				</filter>
