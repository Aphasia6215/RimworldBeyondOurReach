    <!-- Bionic eye -->

    <HediffDef ParentName="AddedBodyPartBase">
        <defName>BOR_TenebralBionicEye</defName>
        <label>tenebral bionic eye</label>
        <labelNoun>a tenebral bionic eye</labelNoun>
