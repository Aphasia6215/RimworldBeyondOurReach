    <RecipeDef>
        <defName>BOR_Make_QuasarAlloy</defName>
        <label>forge quasar alloy</label>
        <description>Forge quasar alloy from plasteel.</description>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Gold</li>
                    </thingDefs>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Uranium</li>
                    </thingDefs>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>BOR_Sunsteel</li>
                    </thingDefs>
                </filter>
                <count>25</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>Gold</li>
                <li>Uranium</li>
                <li>BOR_Sunsteel</li>
            </thingDefs>
        </fixedIngredientFilter>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <workAmount>3375</workAmount>
        <soundWorking>Recipe_Machining</soundWorking>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <skillRequirements>
            <Crafting>15</Crafting>
        </skillRequirements>
        <jobString>Forging Quasar Alloy</jobString>
        <recipeUsers>
            <li>BOR_Bench_II</li>
<li>BOR_Bench_IV</li>
        </recipeUsers>
        <products>
            <BOR_QuasarAlloy>25</BOR_QuasarAlloy>
        </products>
        <researchPrerequisite>BOR_Research_Production_II</researchPrerequisite>
    </RecipeDef>

