<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ResearchProjectDef[@Name="BOR_Research_Base"]/requiredResearchFacilities</xpath>
    <value>
      <li>Rimputer_quantumcomputer</li>
    </value>
  </Operation>

  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ResearchProjectDef[defName="Rimputer_quantumcomputer"]/requiredResearchFacilities</xpath>
    <value>
      <li>RP_SuperComputer</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ResearchProjectDef[defName="Rimputer_supercomputer"]/requiredResearchFacilities</xpath>
    <value>
      <li>RP_Server</li>
    </value>
  </Operation>
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/ResearchProjectDef[defName="Rimputer_server"]</xpath>
    <value>
      <requiredResearchFacilities>
        <li>RP_TapeStorage</li>
      </requiredResearchFacilities>

    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/texPath</xpath>
    <value>
      <texPath>BOR/Retextures/quantumcomputer</texPath>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Rimputer_quantumcomputer"]/graphicData/graphicClass</xpath>
    <value>
      <graphicClass>Graphic_Single</graphicClass>
    </value>
  </Operation>

  <Operation Class="PatchOperationReplace">
    <xpath>/Defs/ThingDef[defName="Rimputer_quantumcomputer"]/rotatable</xpath>
    <value>
      <rotatable>False</rotatable>
    </value>
  </Operation>
</Patch>