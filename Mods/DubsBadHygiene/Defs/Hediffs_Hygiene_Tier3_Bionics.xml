<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<HediffDef ParentName="ImplantHediffBase">
		<defName>EnigmaticBladder</defName>
		<label>enigmatic bladder</label>
		<labelNoun>a enigmatic bladder</labelNoun>
		<description>An installed enigmatic bladder.</description>
		<descriptionHyperlinks>
			<ThingDef>EnigmaticBladder</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>EnigmaticBladder</spawnThingOnRemoved>
		<stages>
			<li>
				<statOffsets>
					<BladderRateMultiplier>-0.85</BladderRateMultiplier>
					<ThirstRateMultiplier>-0.85</ThirstRateMultiplier>
				</statOffsets>
			</li>
		</stages>
	</HediffDef>

	<ThingDef ParentName="BodyPartBionicBase">
		<defName>EnigmaticBladder</defName>
		<label>enigmatic bladder</label>
		<description>An advanced enigmatic-tech bladder. A chemical recycling system breaks down waste products from the body into molecules which are recycled with the remainder released as gas, the downside of this being that it gives the user gas.</description>
		<descriptionHyperlinks>
			<RecipeDef>InstallEnigmaticBladder</RecipeDef>
		</descriptionHyperlinks>
		<costList>
			<BOR_Tenebrite>10</BOR_Tenebrite>
			<BOR_ComponentIII>3</BOR_ComponentIII>
		</costList>
		<statBases>
			<Mass>2</Mass>
			<WorkToMake>30000</WorkToMake>
			<MarketValue>30000</MarketValue>
		</statBases>
		<recipeMaker>
			<skillRequirements>
				<Crafting>18</Crafting>
			</skillRequirements>
			<researchPrerequisite MayRequire="dbhlitemode">HygieneBionics</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>Table_SNS_BionicCraftingStation_GenIII</li>
			</recipeUsers>
		</recipeMaker>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallImplantBase">
		<defName>InstallEnigmaticBladder</defName>
		<label>install enigmatic bladder</label>
		<description>Install a enigmatic bladder.</description>
		<skillRequirements>
			<Medicine>18</Medicine>
		</skillRequirements>
		<descriptionHyperlinks>
			<ThingDef>EnigmaticBladder</ThingDef>
			<HediffDef>EnigmaticBladder</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing enigmatic bladder.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>EnigmaticBladder</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BOR_MedicineIII</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>BOR_Femtoclusters</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BOR_MedicineIII</li>
				<li>BOR_Femtoclusters</li>
				<li>EnigmaticBladder</li>
			</thingDefs>
		</fixedIngredientFilter>
		<addsHediff>EnigmaticBladder</addsHediff>
		<appliedOnFixedBodyParts>
			<li>Torso</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>


