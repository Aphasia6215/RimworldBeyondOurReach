<?xml version="1.0" encoding="UTF-8"?>
<Defs>

  <PipeSystem.ProcessDef Abstract="True" Name="BOR_ProcessGenIWasteful">
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
    <wastePackToProduce>1000</wastePackToProduce>
    <researchPrerequisites>
      <li>BOR_Research_Refinery_I_Wasteful</li>
    </researchPrerequisites>
  </PipeSystem.ProcessDef>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIWasteful">
    <defName>BOR_QuasarMatterIntoSteelGenIWasteful</defName>
    <label>Fabricate Steel from Quasar Matter in a wasteful process.</label>
    <ingredients>
      <li>
        <pipeNet>BOR_QuasarMatterNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>50</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>150000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


