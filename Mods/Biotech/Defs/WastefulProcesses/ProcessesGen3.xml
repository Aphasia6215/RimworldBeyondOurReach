  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIIIWasteful">
    <defName>BOR_NeutroniumIntoSteelGenIIIWasteful</defName>
    <label>Fabricate Steel from Neutronium in a wasteful process.</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>2000</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>75000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


