  <PipeSystem.ProcessDef Abstract="True" Name="BOR_ProcessGenIIWasteful">
    <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
    <wastePackToProduce>2500</wastePackToProduce>
    <researchPrerequisites>
      <li>BOR_Research_Refinery_II_Wasteful</li>
    </researchPrerequisites>
  </PipeSystem.ProcessDef>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIIWasteful">
    <defName>BOR_QuasarMatterIntoSteelGenIIWasteful</defName>
    <label>Fabricate Steel from Quasar Matter in a wasteful process.</label>
    <ingredients>
      <li>
        <pipeNet>BOR_QuasarMatterNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>30</countNeeded>
      </li>
      <li>
        <thing>Steel</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>100000</ticks>
    <results>
      <li>
        <thing>Steel</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


