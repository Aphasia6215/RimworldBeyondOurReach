<?xml version="1.0" encoding="UTF-8"?>
<Defs>

  <PipeSystem.ProcessDef ParentName="BOR_ProcessGenIWasteful" MayRequire="mlie.infinityrimariadna">
    <defName>BOR_NeutroniumIntoTeseumGenIWasteful</defName>
    <label>Fabricate Teseum from Neutronium</label>
    <ingredients>
      <li>
        <pipeNet>BOR_NeutroniumNet</pipeNet>
        <thing>BOR_Neutronium</thing>
        <countNeeded>1000</countNeeded>
      </li>
      <li>
        <thing>Teseum</thing>
        <countNeeded>1</countNeeded>
      </li>
    </ingredients>
    <ticks>300000</ticks>
    <results>
      <li>
        <thing>Teseum</thing>
        <count>500</count>
      </li>
    </results>
  </PipeSystem.ProcessDef>


