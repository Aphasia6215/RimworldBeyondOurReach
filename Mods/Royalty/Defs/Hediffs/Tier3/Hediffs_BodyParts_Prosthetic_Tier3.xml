    <fixedIngredientFilter>
      <thingDefs>
        <li>MindscrewEnigmatic</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MindscrewEnigmatic</addsHediff>
