<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- Mindscrew -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>MindscrewTenebral</defName>
    <label>tenebral mindscrew</label>
    <labelNoun>an tenebral mindscrew</labelNoun>
    <description>An installed tenebral mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewTenebral</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>MindscrewTenebral</spawnThingOnRemoved>
    <priceOffset>-440</priceOffset>
    <stages>
      <li>
        <painOffset>0.6</painOffset>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartBORTenebralBase">
    <defName>MindscrewTenebral</defName>
    <label>tenebral mindscrew</label>
    <description>Inflicts continuous pain on the user. Mindscrews are used by some authorities as a long-term punishment. Some even install them voluntarily as a demonstration of ideological or religious devotion.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallMindscrewTenebral</RecipeDef>
    </descriptionHyperlinks>
    <techHediffsTags>
      <li>PainCauser</li>
    </techHediffsTags>

  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantBORTenebralBase">
    <defName>InstallMindscrewTenebral</defName>
    <label>install tenebral mindscrew</label>
    <description>Install a mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewTenebral</ThingDef>
      <HediffDef>MindscrewTenebral</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing tenebral mindscrew.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MindscrewTenebral</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MindscrewTenebral</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MindscrewTenebral</addsHediff>
  </RecipeDef>

  <!-- Drill arm -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>DrillArmTenebral</defName>
    <label>tenebral drill arm</label>
    <labelNoun>an tenebral drill arm</labelNoun>
    <description>An installed tenebral drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmTenebral</ThingDef>
    </descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <MiningSpeed>6.4</MiningSpeed>
          <DeepDrillingSpeed>2.8</DeepDrillingSpeed>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.12</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>tenebral drill</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>52</power>
            <cooldownTime>3</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
            <soundMeleeHit>MeleeHit_DrillArm</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_DrillArm</soundMeleeMiss>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>DrillArmTenebral</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="BodyPartBORTenebralBase">
    <defName>DrillArmTenebral</defName>
    <label>tenebral drill arm</label>
    <description>A mechanical arm replacement for speeding up mining. It has a built-in snake drill, which allows the miner to rapidly find and exploit weak lines in the rock. It also has a general-purpose manipulator. It is somewhat ungainly, which slows down movement slightly.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallDrillArmTenebral</RecipeDef>
    </descriptionHyperlinks>

    <techHediffsTags>
      <li>DrillArm</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
    <defName>InstallDrillArmTenebral</defName>
    <label>install tenebral drill arm</label>
    <description>Install a drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmTenebral</ThingDef>
      <HediffDef>DrillArmTenebral</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing tenebral drill arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>DrillArmTenebral</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>DrillArmTenebral</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
    <addsHediff>DrillArmTenebral</addsHediff>
  </RecipeDef>

