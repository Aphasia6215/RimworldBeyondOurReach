<Defs>

  <!-- Mindscrew -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>MindscrewStellar</defName>
    <label>stellar mindscrew</label>
    <labelNoun>a stellar mindscrew</labelNoun>
    <description>An installed stellar mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewStellar</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>MindscrewStellar</spawnThingOnRemoved>
    <priceOffset>-220</priceOffset>
    <stages>
      <li>
        <painOffset>0.4</painOffset>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartAOTCStellarBase">
    <defName>MindscrewStellar</defName>
    <label>stellar mindscrew</label>
    <description>Inflicts continuous pain on the user. Mindscrews are used by some authorities as a long-term punishment. Some even install them voluntarily as a demonstration of ideological or religious devotion.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallMindscrewStellar</RecipeDef>
    </descriptionHyperlinks>
    <techHediffsTags>
      <li>PainCauser</li>
    </techHediffsTags>

  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantAOTCStellarBase">
    <defName>InstallMindscrewStellar</defName>
    <label>install stellar mindscrew</label>
    <description>Install a mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewStellar</ThingDef>
      <HediffDef>MindscrewStellar</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing stellar mindscrew.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MindscrewStellar</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MindscrewStellar</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MindscrewStellar</addsHediff>
  </RecipeDef>

  <!-- Drill arm -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>DrillArmStellar</defName>
    <label>stellar drill arm</label>
    <labelNoun>a stellar drill arm</labelNoun>
    <description>An installed stellar drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmStellar</ThingDef>
    </descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <MiningSpeed>3.20</MiningSpeed>
          <DeepDrillingSpeed>1.4</DeepDrillingSpeed>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.06</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>stellar drill</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>26</power>
            <cooldownTime>2</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
            <soundMeleeHit>MeleeHit_DrillArm</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_DrillArm</soundMeleeMiss>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>DrillArmStellar</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="BodyPartAOTCStellarBase">
    <defName>DrillArmStellar</defName>
    <label>stellar drill arm</label>
    <description>A mechanical arm replacement for speeding up mining. It has a built-in snake drill, which allows the miner to rapidly find and exploit weak lines in the rock. It also has a general-purpose manipulator. It is somewhat ungainly, which slows down movement slightly.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallDrillArmStellar</RecipeDef>
    </descriptionHyperlinks>

    <techHediffsTags>
      <li>DrillArm</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
    <defName>InstallDrillArmStellar</defName>
    <label>install stellar drill arm</label>
    <description>Install a drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmStellar</ThingDef>
      <HediffDef>DrillArmStellar</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing stellar drill arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>DrillArmStellar</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>DrillArmStellar</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
    <addsHediff>DrillArmStellar</addsHediff>
  </RecipeDef>

