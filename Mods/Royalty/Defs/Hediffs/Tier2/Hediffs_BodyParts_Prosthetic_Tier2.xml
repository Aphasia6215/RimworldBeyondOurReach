<Defs>

  <!-- Mindscrew -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>MindscrewCosmic</defName>
    <label>cosmic mindscrew</label>
    <labelNoun>a cosmic mindscrew</labelNoun>
    <description>An installed cosmic mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewCosmic</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>MindscrewCosmic</spawnThingOnRemoved>
    <priceOffset>-440</priceOffset>
    <stages>
      <li>
        <painOffset>0.6</painOffset>
      </li>
    </stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartAOTCCosmicBase">
    <defName>MindscrewCosmic</defName>
    <label>cosmic mindscrew</label>
    <description>Inflicts continuous pain on the user. Mindscrews are used by some authorities as a long-term punishment. Some even install them voluntarily as a demonstration of ideological or religious devotion.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallMindscrewCosmic</RecipeDef>
    </descriptionHyperlinks>
    <techHediffsTags>
      <li>PainCauser</li>
    </techHediffsTags>

  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantAOTCCosmicBase">
    <defName>InstallMindscrewCosmic</defName>
    <label>install cosmic mindscrew</label>
    <description>Install a mindscrew.</description>
    <descriptionHyperlinks>
      <ThingDef>MindscrewCosmic</ThingDef>
      <HediffDef>MindscrewCosmic</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing cosmic mindscrew.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MindscrewCosmic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MindscrewCosmic</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MindscrewCosmic</addsHediff>
  </RecipeDef>

  <!-- Drill arm -->

  <HediffDef ParentName="AddedBodyPartBase">
    <defName>DrillArmCosmic</defName>
    <label>cosmic drill arm</label>
    <labelNoun>a cosmic drill arm</labelNoun>
    <description>An installed cosmic drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmCosmic</ThingDef>
    </descriptionHyperlinks>
    <addedPartProps>
      <solid>true</solid>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <MiningSpeed>6.4</MiningSpeed>
          <DeepDrillingSpeed>2.8</DeepDrillingSpeed>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.12</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>cosmic drill</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>52</power>
            <cooldownTime>3</cooldownTime>
            <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
            <soundMeleeHit>MeleeHit_DrillArm</soundMeleeHit>
            <soundMeleeMiss>MeleeMiss_DrillArm</soundMeleeMiss>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>DrillArmCosmic</spawnThingOnRemoved>
  </HediffDef>

  <ThingDef ParentName="BodyPartAOTCCosmicBase">
    <defName>DrillArmCosmic</defName>
    <label>cosmic drill arm</label>
    <description>A mechanical arm replacement for speeding up mining. It has a built-in snake drill, which allows the miner to rapidly find and exploit weak lines in the rock. It also has a general-purpose manipulator. It is somewhat ungainly, which slows down movement slightly.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallDrillArmCosmic</RecipeDef>
    </descriptionHyperlinks>

    <techHediffsTags>
      <li>DrillArm</li>
    </techHediffsTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
    <defName>InstallDrillArmCosmic</defName>
    <label>install cosmic drill arm</label>
    <description>Install a drill arm.</description>
    <descriptionHyperlinks>
      <ThingDef>DrillArmCosmic</ThingDef>
      <HediffDef>DrillArmCosmic</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing cosmic drill arm.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>DrillArmCosmic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>DrillArmCosmic</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Arm</li>
    </appliedOnFixedBodyParts>
