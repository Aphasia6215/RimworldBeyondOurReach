  <!-- Gastro-analyzer -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>GastroAnalyzerCosmic</defName>
    <label>cosmic gastro-analyzer</label>
    <labelNoun>a cosmic gastro-analyzer</labelNoun>
    <description>An installed cosmic gastro-analyzer.</description>
    <descriptionHyperlinks>
