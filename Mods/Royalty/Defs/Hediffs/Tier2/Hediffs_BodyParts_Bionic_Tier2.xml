<Defs>

  <!-- Neurocalculator -->

  <HediffDef ParentName="ImplantHediffBase">
    <defName>NeurocalculatorCosmic</defName>
    <label>cosmic neurocalculator</label>
    <labelNoun>a cosmic neurocalculator</labelNoun>
    <description>An installed cosmic neurocalculator.</description>
    <descriptionHyperlinks>
      <ThingDef>NeurocalculatorCosmic</ThingDef>
    </descriptionHyperlinks>
    <spawnThingOnRemoved>NeurocalculatorCosmic</spawnThingOnRemoved>
    <stages>
      <li>
        <statOffsets>
          <ResearchSpeed>0.6</ResearchSpeed>
        </statOffsets>
      </li>
    </stages>
    <comps>
      <li Class="HediffCompProperties_ReactOnDamage">
        <damageDefIncoming>EMP</damageDefIncoming>
        <createHediff>BrainShock</createHediff>
      </li>
    </comps>
  </HediffDef>

  <ThingDef ParentName="BodyPartAOTCCosmicBase">
    <defName>NeurocalculatorCosmic</defName>
    <label>cosmic neurocalculator</label>
    <description>A non-intelligent microcomputer controlled by a visual/tactile neural interface. It assists with memory and calculation, like a calculator and notepad embedded in the brain.</description>
    <descriptionHyperlinks>
      <RecipeDef>InstallNeurocalculatorCosmic</RecipeDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Item/Health/HealthItem</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.80</drawSize>
      <color>(189,169,118)</color>
    </graphicData>

  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallImplantAOTCCosmicBase">
    <defName>InstallNeurocalculatorCosmic</defName>
    <label>install cosmic neurocalculator</label>
    <description>Install a neurocalculator.</description>
    <descriptionHyperlinks>
      <ThingDef>NeurocalculatorCosmic</ThingDef>
      <HediffDef>NeurocalculatorCosmic</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing cosmic neurocalculator.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>NeurocalculatorCosmic</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>NeurocalculatorCosmic</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Brain</li>
    </appliedOnFixedBodyParts>
    <addsHediff>NeurocalculatorCosmic</addsHediff>
  </RecipeDef>

  <RecipeDef ParentName="SurgeryRemoveImplantBase">
    <defName>RemoveNeurocalculatorCosmic</defName>
    <label>cosmic remove neurocalculator</label>
    <description>Remove neurocalculator.</description>
    <descriptionHyperlinks>
      <ThingDef>NeurocalculatorCosmic</ThingDef>
      <HediffDef>NeurocalculatorCosmic</HediffDef>
    </descriptionHyperlinks>
    <jobString>Removing cosmic neurocalculator.</jobString>
    <removesHediff>NeurocalculatorCosmic</removesHediff>
  </RecipeDef>

